<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PopUpScreens.ControllerDeleteItemWindow">
   <children>
      <VBox alignment="CENTER" prefWidth="300.0" spacing="20.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
         <children>
            <HBox alignment="CENTER" prefWidth="200.0">
               <children>
                  <Label minHeight="-Infinity" minWidth="-Infinity" text="Voulez vous vraiment supprimer : " />
                  <Label fx:id="LabelItemToDelete" text="Label" wrapText="true" />
               </children>
            </HBox>
            <Button fx:id="btnDeleteItem" mnemonicParsing="false" onAction="#handleClicks" style="-fx-background-color: transparent; -fx-border-radius: 20; -fx-border-color: #ff1c1c;" stylesheets="@ModifyScreenStyle.css" text="Supprimer" textFill="#ff1c1c" />
         </children>
      </VBox>
   </children>
</AnchorPane>

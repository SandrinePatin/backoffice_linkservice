<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="home.DisconnectionConfirm">
   <children>
      <VBox spacing="10.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
         <children>
            <Label text="Voulez vous vraiment vous deconnecter ? " />
            <HBox alignment="CENTER" spacing="30.0">
               <children>
                  <Button fx:id="btnYes" mnemonicParsing="false" onAction="#handleClicks" text="Oui" />
                  <Button fx:id="btnNo" mnemonicParsing="false" onAction="#handleClicks" text="Non" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
